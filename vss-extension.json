{
    "manifestVersion": 1,
    "id": "board-stack",
    "publisher": "elevenlabs",
    "version": "1.0.0",
    "name": "Stack Board",
    "description": "Stack board extension for Azure DevOps",
    "public": false,
    "categories": ["Azure Repos"],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "icons": {
        "default": "marketplace/icon-light.png",
        "branding": "marketplace/icon-light.png"
    },
    "content": {
        "details": {
            "path": "marketplace/overview.md"
        },
        "license": {
            "path": "marketplace/license.md"
        }
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/eleven-financial/board-stack-extension"
    },
    "links": {
        "support": {
            "url": "mailto:luiz.lima@elevenfinancial.com"
        }
    },
    "contributions": [
        {
            "id": "board-stack-hub",
            "type": "ms.vss-web.hub",
            "targets": [
                "ms.vss-code-web.code-hub-group"
            ],
            "properties": {
                "name": "Stack Board",
                "uri": "build/index.html",
                "icon": {
                    "light": "marketplace/icon-light.png",
                    "dark": "marketplace/icon-dark.png"
                }
            }
        }
    ],
    "files": [
        {
            "path": "marketplace",
            "addressable": true
        },
        {
            "path": "build",
            "addressable": true
        }
    ]
}